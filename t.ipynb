   "metadata": {}
   ],
   "source": [
    "from semopy import Model\n",
    "mod = \"iweight ~ rainfall + temperature + humidity\"\n",
    "model = Model(mod)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [],
   "source": [
    "from pandas import read_csv\n",
    "data = read_csv(\"data.csv\", index_col=0)\n",
    "model.load_dataset(data)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "metadata": {}
   ],
   "source": [
    "from semopy import Optimizer\n",
    "opt = Optimizer(model)\n",
    "objective_function_value = opt.optimize()"
